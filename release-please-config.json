{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "release-type": "node",
  "include-component-in-tag": false,
  "packages": {
    ".": {
      "release-type": "node"
    }
  },
  "group-pull-request-title-pattern": "chore(release): release ${version}",
  "pull-request-title-pattern": "chore(release): release ${version}",
  "changelog-sections": [
    {
      "type": "feat",
      "section": "Features",
      "hidden": false
    },
    {
      "type": "fix",
      "section": "Bug Fixes",
      "hidden": false
    },
    {
      "type": "chore",
      "section": "Chore",
      "hidden": false
    },
    {
      "type": "build",
      "section": "Builds",
      "hidden": false
    },
    {
      "type": "docs",
      "section": "Docs",
      "hidden": false
    },
    {
      "type": "style",
      "section": "Coding Style",
      "hidden": false
    },
    {
      "type": "refactor",
      "section": "Refactor",
      "hidden": false
    },
    {
      "type": "perf",
      "section": "Performance",
      "hidden": false
    },
    {
      "type": "test",
      "section": "Test Code",
      "hidden": false
    }
  ]
}